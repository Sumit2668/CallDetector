<?xml version="1.0" encoding="UTF-8" ?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#ff7ac741" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <gradient android:startColor="#807ac741" android:endColor="#808bd554" android:angle="90.0" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff7ac741" android:endColor="#ff8bd554" android:angle="90.0" />
            <corners android:radius="8dp" />
        </shape>
    </item>
</selector>


